@model IEnumerable<TownOfStettler.Models.History>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Wiped)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PartsRemoved)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateOfChanges)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OutOfServiceDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Notes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Device)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PartsChangedNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PastOwnerSNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Wiped)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PartsRemoved)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateOfChanges)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OutOfServiceDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Notes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Device.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PartsChangedNavigation.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PastOwnerSNavigation.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
